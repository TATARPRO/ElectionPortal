
@{
    ViewBag.Title = "Import RegNumbers";
}

<h2>ExcelRegNumbers</h2>
<div class="row">
    <div class="col-lg-3"></div>
    <div class="col-lg-6">

@using (Html.BeginForm("ExcelRegNumbers", "RegNumbers", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <h4>@ViewBag.CorruptFile</h4>
    <p>Select file:</p>
    <input type="file" name="upload" id="upload" /><br />
        <input id="sub" type="submit" value="Upload File" />
}
        
    </div>
    <div class="col-lg-3"></div>

</div>

